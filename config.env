# Module configuration variables, required inside docker container

# module detail
MODULE_TYPE=EGRESS
MODULE_NAME=http-egress

# egress url to send data
EGRESS_WEBHOOK_URL=None
EGRESS_URL=None
CONTENT_TYPE_JSON=no
AUTHENTICATION_REQUIRED=no
ACCESS_TOKEN=None
AUTHENTICATION_API_KEY=None
ERROR_URL=None
# port and path of the module
INGRESS_PORT=80
INGRESS_PATH=/


